type Repo {
  id: Int!
  name: String!
  ownerName: String!
  fullName: String!
  repoHtmlUrl: String!
  description: String
  stars: Int!
  language: String
  forks: Int!
  watchers: Int!
  linesOfCode: Int!
  openedIssues: Int!
  score: Int!
}

type Score {
  score: Int!
}

